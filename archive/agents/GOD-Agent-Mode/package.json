{
  "name": "god-agent-mode",
  "version": "0.2.0",
  "description": "Next-level Hyperflow Agent Orchestrator - Personality-driven, turn-based execution for neurodivergent developers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/welshDog/GOD-Agent-Mode"
  },
  "keywords": [
    "agents",
    "orchestration",
    "graph-execution",
    "dataflow",
    "neurodivergent",
    "hyperflow",
    "turn-based"
  ],
  "author": "welshDog (Lyndon Williams)",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
